<table class="almanac">
	<thead>
		<tr>
			<th>[[%temperature? &namespace=`wunderground`]]</th>
			<th>[[%normal? &namespace=`wunderground`]]</th>
			<th>[[%record? &namespace=`wunderground`]]</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>[[%high_temperature? &namespace=`wunderground`]]</td>
			<td>[[+almanac.temp_high.normal.F]] &#8457;</td>
			<td>[[+almanac.temp_high.record.F]] &#8457; <span class="record">[[+almanac.temp_high.recordyear]]</span></td>
		</tr>
		<tr>
			<td>[[%low_temperature? &namespace=`wunderground`]]</td>
			<td>[[+almanac.temp_low.normal.F]] &#8457;</td>
			<td>[[+almanac.temp_low.record.F]] &#8457; <span class="record">[[+almanac.temp_low.recordyear]]</span></td>
		</tr>
	</tbody>
</table>